<div class = "container">
  <div class = "row">
    <div class= "mt-4">
      <h4>ジャンル一覧•追加</h4>
      <%= form_with model: [:admin, @genre_new],url: genres_path, method: :post ,local: true do |f| %>
        <%= f.label :ジャンル名 %>
        <%= f.text_field :name %>
        <%= f.submit "新規登録", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>